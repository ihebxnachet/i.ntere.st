<% layout('layout') %>
<div class="container">
<div class="row">
    <div class="col-lg-10">
        <h1>Create a new pin</h1>
        <form action="" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="Title">Title:</label>
            <input type="text" class="form-control" name="Title"  />
        </div>
        <div class="form-group">
            <label for="Desc">Description:</label>
            <textarea type="text" class="form-control" name="Desc"></textarea>
        </div>
        <div class="form-group">
            <label for="Username">Created By:</label>
            <input type="text" class="form-control" name="Username"  />
        </div>
        <div class="form-group">
            <label for="sampleFile">image Path:</label>
            <input type="file" class="form-control" name="sampleFile"  />
        </div>
        <button type="submit" class="btn btn-success">submit</button>
        </form>
    </div>

</div>
</div>